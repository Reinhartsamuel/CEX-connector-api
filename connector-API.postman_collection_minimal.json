            "body": {
              "mode": "raw",
              "raw": "{\n  \"contract\": \"BTC_USDT\",\n  \"size\": 1,\n  \"price\": 50000,\n  \"side\": \"buy\",\n  \"tif\": \"ioc\",\n  \"text\": \"order-001\",\n  \"reduce_only\": false,\n  \"close_position\": false,\n  \"autotrader_id\": \"autotrader-001\",\n  \"user_id\": \"user-001\"\n}"
            },
            "url": {
