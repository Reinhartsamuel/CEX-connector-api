            "body": {
              "mode": "raw",
              "raw": "{\n  \"api_key\": \"{{gateApiKey}}\",\n  \"api_secret\": \"{{gateApiSecret}}\",\n  \"user_id\": \"user-001\",\n  \"exchange_user_id\": \"gate_account_001\"\n}"
            },
